{
	"moduleFileExtensions": [
		"js",
		"json",
		"ts"
	],
	"rootDir": "test",
	"testRegex": ".test.js$",
	"coverageDirectory": "../coverage",
	"testEnvironment": "node",
	"reporters": [
		"default"
	],
	"collectCoverageFrom": [
		"test/*.{js}",
		"!**/node_modules/**",
		"!**/tools/**",
		"!**/build/**",
		"!**/docs/**"
	  ]
}